    "expo-status-bar": "^3.0.8",
    "firebase": "^12.2.1",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-dom": "^19.2.0",
    "react-native": "0.81.4",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-reanimated": "~4.1.3",
    "react-native-safe-area-context": "^5.6.1",
