    "gh-pages": "^6.3.0",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "react-native-svg-transformer": "^1.5.1",
    "tailwindcss": "3.4.17",
    "typescript": "~5.9.3"
